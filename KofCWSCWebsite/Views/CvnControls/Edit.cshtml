@model KofCWSCWebsite.Models.CvnControl

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Convention Location String</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="LocationString" class="control-label"></label>
                <input asp-for="LocationString" class="form-control" />
                <span asp-validation-for="LocationString" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
