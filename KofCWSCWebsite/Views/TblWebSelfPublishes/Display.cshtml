@model IEnumerable<KofCWSCWebsite.Models.SPGetSOS>

@{
    ViewData["Title"] = "Message Details";
}

<div>
    @foreach (var item in Model)
    {
        <h3>@item.FullName</h3>
        <img src="@item.Photo" width="200" />
        @Html.Raw(item.Data);
    }
    <a href="/">Return Home</a>
    @if (User.IsInRole("Admin") || User.IsInRole("StateOfficer"))
    {
        <span>|</span>
        <a asp-action="EditInt" asp-route-id="@Model.FirstOrDefault().OID">Edit</a>
    }
</div>